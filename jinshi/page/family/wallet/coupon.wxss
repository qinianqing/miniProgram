  padding-top: 28rpx;
  padding-left: 55rpx;
  font-family: PingFangSC-Regular;
  color: rgba(180, 176, 176, 1);
  font-size: 26rpx;
  box-sizing: border-box;
  /* margin-bottom: 74rpx; */
