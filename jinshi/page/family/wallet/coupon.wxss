}

.code {
  width: 750rpx;
  display: flex;
  flex-direction: row;
}

.code-input {
  box-shadow: 0 8rpx 14rpx 0 rgba(255, 108, 90, 0.07);
  border-radius: 100rpx;
  line-height: 100rpx;
  background-size: cover;
  width: 480rpx;
  height: 100rpx;
