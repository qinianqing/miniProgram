/* 占位图  */

.place-holder {
  margin-top: 310rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.place-holder-img {
  width: 213rpx;
  height: 310rpx;
}

.place-holder-txt {
  width: 100%;
  text-align: center;
  margin-top: 60rpx;
  font-family: PingFangSC-Light;
  font-size: 28rpx;
  color: #838383;
  letter-spacing: 0;
}

page {
  background-color: white;
}

.container-box-iphonex{
  padding-top: 182rpx;
  width: 100%;
}

.container-box{
  padding-top: 132rpx;
  width: 100%;
}

.iphonex-blank {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 50rpx;
  background: #fff;
}

.head {
  width: 750rpx;
  height: 132rpx;
  position: fixed;
  top: 0;
  left: 0;
  background: white;
  box-sizing: border-box;
}

.head-iphonex{
  padding-top: 50rpx;
  width: 750rpx;
  height: 132rpx;
  position: fixed;
  top: 50rpx;
  left: 0;
  background: white;
  box-sizing: border-box;
}

.back{
  position: absolute;
  top: 0;
  left: 0;
  z-index: 9;
}

.nav {
  width: 19.6rpx;
  height: 32.7rpx;
  padding: 64rpx 234.9rpx 27.3rpx 41rpx;
}

.head-title {
  z-index: 8;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  text-align: center;
  height: 45rpx;
  padding-top: 59rpx;
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #303030;
}

.banner {
  width: 750rpx;
  height: 100rpx;
  text-align: center;
  line-height: 100rpx;
  display: flex;
  flex-direction: row;
}

.banner-use {
  width: 168rpx;
  /* margin: 0rpx 185rpx 29rpx 133rpx; */
  margin-right: 185rpx;
  margin-left: 133rpx;
}

.banner-unuse {
  width: 168rpx;
}

.f {
  color: rgba(254, 149, 137, 1);
  font-size: 32rpx;
