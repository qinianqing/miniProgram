  border-top: 2rpx solid #e6e6e6;
}

.info-title {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #2d2d2d;
  letter-spacing: 0;
  margin-left: 50rpx;
  width: 158.8rpx;
}

.info-input {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #2d2d2d;
  letter-spacing: 0;
  margin-left: 35.2rpx;
  height: 130rpx;
  line-height: 130rpx;
  width: 500rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.info-input-a {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
