.info-input-b {
  font-family: PingFangSC-Medium;
  font-size: 32rpx;
  color: #2d2d2d;
  letter-spacing: 0;
 
  margin-left: 35.2rpx;
  min-height: 65px;
   line-height: 130rpx; 
  width: 500rpx;
  padding-top: 45rpx;
}

#text {
  font-family: PingFangSC-Regular;
  font-size: 32rpx;
  color: #888;
  letter-spacing: 0;
}

