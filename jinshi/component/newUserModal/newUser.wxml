<template name="newUser">
  <view class='new-user' bindtap='newUserClose'>
    <view class='new-user-box'>
      <view class='new-user-top'>
        <view class='nut-title'>已获新人券</view>
        <image class='nut-background' src='https://cdn.jiyong365.com/%E5%B7%B2%E8%8E%B7%E6%96%B0%E4%BA%BA%E5%88%B8-04.png'></image>
        <image class='nut-quan' src='https://cdn.jiyong365.com/%E5%B7%B2%E8%8E%B7%E6%96%B0%E4%BA%BA%E5%88%B8-05.png' catchtap='goCheckCoupon'></image>
        <image class='nut-quan-mask' src='https://cdn.jiyong365.com/%E5%B7%B2%E8%8E%B7%E6%96%B0%E4%BA%BA%E5%88%B8-06.png'></image>
        <view class='nut-quan-title' catchtap='goCheckCoupon'>立即查看</view>
        <view class='nut-quan-money' catchtap='goCheckCoupon'>{{quan.price}}</view>
        <view class='nut-quan-yuan'>元</view>
        <view class='nut-quan-limit'>满{{quan.limit}}元可用</view>
        <view class='nut-quan-expired'>{{quan.expiredAt}}到期</view>
      </view>
      <view class='new-user-bottom' catchtap=''>
        <image class='nub-info' src='https://cdn.jiyong365.com/%E5%B7%B2%E8%8E%B7%E6%96%B0%E4%BA%BA%E5%88%B8-02.png'></image>
        <view class='nub-title'>新手指南</view>
        <view class='nub-go' catchtap='goGuide'>
          <image src='https://cdn.jiyong365.com/%E5%B7%B2%E8%8E%B7%E6%96%B0%E4%BA%BA%E5%88%B8-03.png'></image>
          <view>去看看</view>
        </view>
      </view>
      <image class='new-user-close' src='https://cdn.jiyong365.com/%E5%8F%96%E6%B6%88%E6%8C%89%E9%92%AE-07.png' catchtap='newUserClose'></image>
    </view>
  </view>
</template>